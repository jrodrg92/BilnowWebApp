        <input type="text" id="raza_Mascota" class="fadeIn second" name="raza_Mascota" placeholder="raza">
        <input type="text" id="capa_Mascota" class="fadeIn second" name="capa_Mascota" placeholder="skin color">
        <input type="text" id="datepicker" class="fadeIn second" name="datepicker" placeholder="fecha_Nacimiento">
        <input type="text" id="sexo_Mascota	" class="fadeIn second" name="sexo_Mascota" placeholder="sexo(Numeric) 1 M / 0 F">
