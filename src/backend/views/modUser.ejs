<% include partials/headerMain%>
<div class="wrapper fadeInDown">
    <div id="formContent">
      <!-- Tabs Titles -->
  
      <!-- Icon -->
      <div class="fadeIn first">
        <img src="/frontend/pictures/logo.gif" id="icon" alt="User Icon" />
      </div>
  
      <form id="container" action='/modUser' method='POST'>
        <div class="form-group">
          <input type="text" id="nom_Usuario" class="fadeIn second" name="nom_Usuario" placeholder="<%=nom_Usuario%>">
          <input type="text" id="ap_Usuario" class="fadeIn second" name="ap_Usuario" placeholder="<%=ap_Usuario%>">
          <input type="text" id="tlf_Usuario" class="fadeIn second" name="tlf_Usuario" placeholder="<%=tlf_Usuario%>">
          <input type="text" id="dir_Usuario" class="fadeIn second" name="dir_Usuario" placeholder="<%=dir_Usuario%>">      
          <input type="text" id="email_Usuario" class="fadeIn second" name="email_Usuario" placeholder="<%=email_Usuario%>">
          <input type="password" id="pswd_Usuario" class="fadeIn second" name="pswd_Usuario" placeholder="password antiguo">
          <input type="password" id="pswd_Usuario1" class="fadeIn second" name="pswd_Usuario1" placeholder="password nuevo">
          <input type="submit" class="fadeIn fourth" value="Mod user">
        </div>
      </form>
    
    </div>
</div>
<% include partials/footerMain%>
